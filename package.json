{
    "name": "proshop",
    "version": "1.0.0",
    "description": "MERN shopping cart app",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node backend/server",
        "server": "nodemon backend/server",
        "client": "npm start --prefix frontend",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "data:import": "node backend/config/seeder",
        "data:destroy": "node backend/config/seeder -d"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/filipeqs/proshop.git"
    },
    "author": "Filipe Silva",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/filipeqs/proshop/issues"
    },
    "homepage": "https://github.com/filipeqs/proshop#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.12.3"
    },
    "devDependencies": {
        "concurrently": "^6.0.1",
        "nodemon": "^2.0.7"
    }
}
